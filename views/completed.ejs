<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta charset="utf-8">
    <title>Completed Tasks</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
<div class="content">
    <h1 class="title">Completed Tasks</h1>
    <div class="nav">
        <%- nav %>
    </div>
    <% completedTasks.forEach(task => { %>
        <div class="completed-task-details">
            <p><b>Task Title:</b> <%= task.title %></p>
            <p><b>Task Description:</b> <%= task.description %></p>
            <p><b>Completed By:</b> <%= task.fname %> <%= task.lname %></p>
            <p><b>Expiration Date:</b> <%= task.due_date %></p>
            <p><b>Notes:</b> <%= task.notes %></p>
        </div><br>
    <% }); %>
</div>
</body>
</html>
